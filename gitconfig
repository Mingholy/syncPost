[alias]
    st = status
    sts = git aa && git stash
    co = checkout
    cb = checkout -b
    aa = add .
    c = commit
    ca = commit -a
    cm = commit -m
    cam = commit --amend
    ft = fetch
    bd = branch -D
    sa = stash apply
    pu = !git pom && git pull && git push
    po = push origin -u
    pom  = pull origin master
    cap = ca && git pu
    rb = rebase
    ri = rebase -i
    rc = rebase --continue
    ra = rebase --abort
    bu = branch -u
    com = checkout master
    mr = merge
    lg = log --graph
